{
    "name": "invoiceplane/invoiceplane",
    "description": "InvoicePlane is a self-hosted open source application for managing your invoices, clients and payments",
    "homepage": "https://invoiceplane.com",
    "license": "MIT",
    "type": "project",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "endroid/qr-code": "^4.8.5",
        "guzzlehttp/guzzle": "^7.10.0",
        "moneyphp/money": "^4.8",
        "mpdf/mpdf": "^8.2.7",
        "paragonie/random_compat": "^2.0.21",
        "phpmailer/phpmailer": "^6.12",
        "pocketarc/codeigniter": "3.3",
        "setasign/fpdf": "^1.8.6",
        "setasign/fpdi": "^2.6.4",
        "smhg/sepa-qr-data": "^1.2",
        "stripe/stripe-php": "^14.10",
        "vlucas/phpdotenv": "^5.6.2"
    },
    "require-dev": {
        "filp/whoops": "^2.18.4",
        "laravel/pint": "^1.26.0",
        "squizlabs/php_codesniffer": "^3.13.5",
        "rector/rector": "^2.2.10",
        "symfony/var-dumper": "^6.4.26"
    },
    "scripts": {
        "check": [
            "rector",
            "phpcs",
            "pint"
        ],
        "rector": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/rector process"
        ],
        "phpcs": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpcbf -p --standard=phpcs.xml"
        ],
        "pint": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/pint"
        ]
    }
}
